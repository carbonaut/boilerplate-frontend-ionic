{
  "name": "pwa-boilerplate",
  "version": "0.0.2",
  "author": "Carbonaut",
  "homepage": "",
  "description": "Boilerplate for Carbonaut projects.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --configuration=$CURRENT_ENV",
    "test": "echo \"Error: no tests specified.\"",
    "lint": "eslint --ext .ts --ext .js src/app/",
    "lint:fix": "eslint --ext .ts --ext .js src/app/ --fix",
    "e2e": "ng e2e",
    "storybook": "npm run docs:json && storybook dev -p 6006",
    "build-storybook": "npm run docs:json && storybook build",
    "bundle-report": "ng build --prod --stats-json --configuration=production && webpack-bundle-analyzer www/stats-es2015.json",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d ."
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "repoLibrary": "state",
      "idKey": "_id",
      "plugins": [
        "@ngneat/elf-cli-ng"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@ionic/angular": "^7.0.6",
    "@ionic/core": "^7.0.6",
    "@ngneat/elf": "^2.1.0",
    "@ngneat/elf-cli-ng": "^1.0.0",
    "@ngneat/elf-entities": "^4.4.2",
    "@ngneat/elf-persist-state": "^1.1.5",
    "@ngx-translate/core": "^14.0.0",
    "core-js": "^3.22.8",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.7",
    "ng-otp-input": "^1.7.1",
    "ng2-date-picker": "^15.0.1",
    "rxjs": "^7.5.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.8",
    "@angular/compiler": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@babel/core": "^7.9.0",
    "@compodoc/compodoc": "^1.1.19",
    "@ionic/angular-toolkit": "^6.1.0",
    "@storybook/addon-actions": "^7.0.12",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/angular": "^7.0.12",
    "@storybook/testing-library": "^0.1.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "babel-loader": "^8.1.0",
    "codelyzer": "^6.0.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "html-loader": "^1.1.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.12",
    "to-string-loader": "^1.1.6",
    "ts-node": "~7.0.0",
    "typescript": "~4.8.4",
    "webpack-bundle-analyzer": "^3.7.0"
  }
}
