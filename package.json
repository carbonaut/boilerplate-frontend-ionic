{
  "name": "pwa-boilerplate",
  "version": "0.0.3",
  "author": "Carbonaut",
  "homepage": "",
  "description": "Boilerplate for Carbonaut projects.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=$CURRENT_ENV",
    "test": "echo \"Error: no tests specified.\"",
    "lint": "eslint --ext .ts --ext .js src/ e2e/ playwright.config.ts",
    "lint:fix": "eslint --ext .ts --ext .js src/ e2e/ playwright.config.ts --fix",
    "e2e": "npx playwright test",
    "bundle-report": "ng build --stats-json --configuration=production && webpack-bundle-analyzer www/stats.json",
    "storybook": "ng run app:storybook",
    "build-storybook": "ng run app:build-storybook"
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "repoLibrary": "state",
      "idKey": "_id",
      "plugins": [
        "@ngneat/elf-cli-ng"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@ionic/angular": "^7.4.1",
    "@ionic/core": "^7.0.6",
    "@ngneat/elf": "^2.4.0",
    "@ngneat/elf-cli-ng": "^1.0.0",
    "@ngneat/elf-entities": "^4.5.0",
    "@ngneat/elf-persist-state": "^1.1.5",
    "@ngx-translate/core": "^14.0.0",
    "core-js": "^3.32.2",
    "date-fns": "^2.30.0",
    "ng-otp-input": "^1.9.3",
    "rxjs": "^7.5.0",
    "tslib": "^1.9.0",
    "zone.js": "^0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@babel/core": "^7.23.0",
    "@ionic/angular-toolkit": "^6.1.0",
    "@playwright/test": "^1.38.1",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/angular": "^7.4.5",
    "@storybook/blocks": "^7.4.5",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.11",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "babel-loader": "^8.1.0",
    "codelyzer": "^6.0.1",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.6.14",
    "html-loader": "^1.1.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^2.5.1",
    "storybook": "^7.4.5",
    "to-string-loader": "^1.1.6",
    "ts-node": "~7.0.0",
    "typescript": "^4.9.3",
    "webpack-bundle-analyzer": "^3.7.0"
  }
}
