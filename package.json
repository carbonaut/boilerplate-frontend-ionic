{
  "name": "pwa-boilerplate",
  "version": "0.0.2",
  "author": "Carbonaut",
  "homepage": "",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --configuration=$CURRENT_ENV",
    "test": "echo \"Error: no tests specified.\"",
    "lint": "eslint --ext .ts --ext .js src/app/",
    "lint:fix": "eslint --ext .ts --ext .js src/app/ --fix",
    "e2e": "ng e2e",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "bundle-report": "ng build --prod --stats-json --configuration=production && webpack-bundle-analyzer www/stats-es2015.json",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d ."
  },
  "elf": {
    "cli": {
      "repoTemplate": "class",
      "repoLibrary": "state",
      "idKey": "_id",
      "plugins": [
        "@ngneat/elf-cli-ng"
      ]
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~13.0.2",
    "@angular/core": "~13.0.2",
    "@angular/forms": "~13.0.2",
    "@angular/platform-browser": "~13.0.2",
    "@angular/platform-browser-dynamic": "~13.0.2",
    "@angular/router": "~13.0.2",
    "@ionic-native/core": "^5.36.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^6.1.8",
    "@ionic/core": "^6.1.8",
    "@ngneat/elf": "^2.1.0",
    "@ngneat/elf-cli-ng": "^1.0.0",
    "@ngneat/elf-entities": "^4.4.2",
    "@ngneat/elf-persist-state": "^1.1.5",
    "@ngx-translate/core": "^12.1.2",
    "core-js": "^3.22.8",
    "ng-otp-input": "^1.7.1",
    "ng2-date-picker": "^8.0.0",
    "rxjs": "~7.5.5",
    "tslib": "^1.9.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.0.3",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "^13.0.3",
    "@angular/compiler": "~13.0.2",
    "@angular/compiler-cli": "~13.0.2",
    "@angular/language-service": "~13.0.2",
    "@babel/core": "^7.9.0",
    "@compodoc/compodoc": "^1.1.19",
    "@ionic/angular-toolkit": "^6.1.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/angular": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "babel-loader": "^8.1.0",
    "codelyzer": "^6.0.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "html-loader": "^1.1.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "to-string-loader": "^1.1.6",
    "ts-node": "~7.0.0",
    "typescript": "~4.4.4",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "description": "Boilerplate for Carbonaut projects."
}
